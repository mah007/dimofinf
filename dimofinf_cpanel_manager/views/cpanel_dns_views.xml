
<odoo>
    <record id="view_cpanel_dns_tree" model="ir.ui.view">
        <field name="name">cpanel.dns.tree</field>
        <field name="model">cpanel.dns</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="type"/>
                <field name="value"/>
                <field name="domain"/>
            </tree>
        </field>
    </record>

    <record id="view_cpanel_dns_form" model="ir.ui.view">
        <field name="name">cpanel.dns.form</field>
        <field name="model">cpanel.dns</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="type"/>
                        <field name="value"/>
                        <field name="domain"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
